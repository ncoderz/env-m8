{
  "name": "@ncoderz/env-m8",
  "version": "0.0.1",
  "description": "Where am I? Information about the environment in which your TypeScript or JavaScript is executing.",
  "author": "RA Sewell <richard.sewell@ncoderz.com>",
  "license": "BSD-2-Clause",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/browser/env-m8.global.js",
  "jsdelivr": "./dist/browser/env-m8.global.js",
  "unpkg": "./dist/browser/env-m8.global.js",
  "exports": {
    "./browser": {
      "browser": "./dist/browser/env-m8.global.js",
      "import": "./dist/browser/env-m8.global.js",
      "require": "./dist/browser/env-m8.global.js"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./esm": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cjs": {
      "import": "./dist/index.cjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "init": "node scripts/init.ts",
    "dev": "ENV_M8=dev node bin/dev.ts",
    "dev-deno": "ENV_M8=dev deno -A bin/dev.ts",
    "dev-bun": "ENV_M8=dev bun bin/dev.ts",
    "dev-browser": "npm run build && cp dist/browser/env-m8.global.* test/browser && npx -y serve test/browser",
    "build": "npm run check && tsup",
    "check": "npm run init && npm run typecheck && npm run lint",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}' '*.{json,md}'",
    "format-check": "prettier --check 'src/**/*.{ts,tsx}' '*.{json,md}'",
    "test": "vitest run",
    "test-watch": "vitest",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "clean": "node scripts/clean.ts"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^24.0.0",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": "^11.0.0",
      "onFail": "error"
    }
  },
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "homepage": "https://github.com/ncoderz/env-m8#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncoderz/env-m8.git"
  },
  "bugs": {
    "url": "https://github.com/ncoderz/env-m8/issues"
  },
  "keywords": [
    "browser",
    "environment",
    "version",
    "os",
    "user-agent"
  ],
  "devDependencies": {
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@ncoderz/log-m8": "^1.2.4",
    "@swc/core": "^1.13.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.1.0",
    "eslint": "^9.11.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fs-extra": "^11.3.0",
    "prettier": "^3.3.3",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.35.0",
    "vitest": "^1.6.0"
  }
}
